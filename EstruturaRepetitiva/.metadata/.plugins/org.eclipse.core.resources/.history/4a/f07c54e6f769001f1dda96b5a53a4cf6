import java.util.Scanner;

public class Main01 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int number;
		number = input.nextInt();
		int sum = 0;
		while(true) {
			if(number == 0)
				break;
			sum += number;
		}
		System.out.println(sum);
		input.close();

	}

}
